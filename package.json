{
  "name": "site-builder",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:editor": "npm run dev --workspace=@site-builder/editor",
    "dev:api": "npm run dev --workspace=@site-builder/api",
    "dev:runtime": "npm run dev --workspace=@site-builder/runtime",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  }
}

